<?xml version="1.0"?>
<package format="2">
    <name>integrated_viewer</name>
    <version>1.10.0</version>
    <description>The integrated_viewer package</description>

    <maintainer email="hirabayashi@ertl.jp">hirabayashi</maintainer>

    <license>Apache 2</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_build_flags</buildtool_depend>
    <build_depend>autoware_msgs</build_depend>
    <build_depend>std_msgs</build_depend>
    <build_depend>rviz</build_depend>
    <build_depend>qtbase5-dev</build_depend>
    <build_depend>cv_bridge</build_depend>
    <build_depend>image_transport</build_depend>
    <build_depend>roscpp</build_depend>
    <build_depend>sensor_msgs</build_depend>

    <exec_depend>autoware_msgs</exec_depend>
    <exec_depend>std_msgs</exec_depend>
    <exec_depend>rviz</exec_depend>
    <exec_depend>libqt5-core</exec_depend>
    <exec_depend>libqt5-widgets</exec_depend>
    <exec_depend>cv_bridge</exec_depend>
    <exec_depend>image_transport</exec_depend>
    <exec_depend>roscpp</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>

    <!-- The export tag contains other, unspecified, tags -->
    <export>
        <!-- Other tools can request additional information be placed here -->
        <rviz plugin="${prefix}/rviz_plugin_description.xml"/>
        <build_type>ament_cmake</build_type>
    </export>
</package>
