<?xml version="1.0"?>
<package format="2">
    <name>roi_object_filter</name>
    <version>1.8.0</version>
    <description>Object Sanitizer filters detected objects</description>

    <maintainer email="abrahammonrroy@yahoo.com">amc</maintainer>
    <license>Apache</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_build_flags</buildtool_depend>

    <build_depend>roscpp</build_depend>
    <build_depend>sensor_msgs</build_depend>
    <build_depend>autoware_msgs</build_depend>
    <build_depend>grid_map_ros</build_depend>
    <build_depend>grid_map_cv</build_depend>
    <build_depend>grid_map_msgs</build_depend>
    <build_depend>geometry_msgs</build_depend>
    <build_depend>tf</build_depend>
    <build_depend>rostest</build_depend>
    <build_depend>rosunit</build_depend>

    <exec_depend>roscpp</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>
    <exec_depend>autoware_msgs</exec_depend>
    <exec_depend>grid_map_ros</exec_depend>
    <exec_depend>grid_map_cv</exec_depend>
    <exec_depend>grid_map_msgs</exec_depend>
    <exec_depend>geometry_msgs</exec_depend>
    <exec_depend>tf</exec_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
