<?xml version="1.0"?>
<package format="2">
    <name>waypoint_maker</name>
    <version>1.10.0</version>
    <description>The waypoint_maker package</description>
    <maintainer email="h_ohta@ertl.jp">h_ohta</maintainer>
    <license>Apache 2</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_build_flags</buildtool_depend>

    <build_depend>gnss</build_depend>
    <build_depend>roscpp</build_depend>
    <build_depend>std_msgs</build_depend>
    <build_depend>geometry_msgs</build_depend>
    <build_depend>nav_msgs</build_depend>
    <build_depend>tf</build_depend>
    <build_depend>waypoint_follower</build_depend>
    <build_depend>vector_map</build_depend>
    <build_depend>lane_planner</build_depend>
    <build_depend>autoware_msgs</build_depend>
    <build_depend>message_filters</build_depend>

    <exec_depend>gnss</exec_depend>
    <exec_depend>roscpp</exec_depend>
    <exec_depend>std_msgs</exec_depend>
    <exec_depend>geometry_msgs</exec_depend>
    <exec_depend>nav_msgs</exec_depend>
    <exec_depend>tf</exec_depend>
    <exec_depend>waypoint_follower</exec_depend>
    <exec_depend>vector_map</exec_depend>
    <exec_depend>lane_planner</exec_depend>
    <exec_depend>autoware_msgs</exec_depend>
    <exec_depend>message_filters</exec_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
